{
  "name": "intel-realtime",
  "version": "4.0.0",
  "description": "Realtime communications layer between IML GUI and API",
  "scripts": {
    "test": "JASMINE_CONFIG_PATH=jasmine.json NODE_ENV=test jasmine",
    "integration": "JASMINE_CONFIG_PATH=jasmine.json NODE_ENV=test jasmine test/integration/**/*-test.js",
    "unit": "JASMINE_CONFIG_PATH=jasmine.json NODE_ENV=test jasmine test/unit/**/*-test.js",
    "jshint": "jshint --c ./.jshintrc --exclude-path ./.jshintignore .",
    "jscs": "jscs ."
  },
  "pre-commit": [
    "jscs",
    "jshint",
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.intel.com/IML/realtime.git"
  },
  "publishConfig": {
    "registry": "https://ubit-artifactory-or.intel.com/artifactory/api/npm/iml-npm-local"
  },
  "author": "IML Team",
  "license": "MIT",
  "dependencies": {
    "intel-fp": "3.2.1",
    "intel-obj": "3.0.0",
    "intel-req": "1.0.6",
    "intel-router": "3.0.4",
    "intel-srcmap-reverse": "1.1.2",
    "intel-through": "1.0.8",
    "bunyan": "1.5.1",
    "clone": "1.0.2",
    "highland": "2.5.1",
    "json-mask": "0.3.5",
    "jsonschema": "1.0.2",
    "nconf": "0.8.2",
    "socket.io": "1.3.7"
  },
  "devDependencies": {
    "intel-stub-daddy": "1.0.7",
    "jasmine": "2.4.1",
    "intel-jasmine-n-matchers": "2.1.1",
    "jscs": "2.6.0",
    "jshint": "2.8.0",
    "krusty-jasmine-reporter": "1.0.0",
    "pre-commit": "1.1.2",
    "proxyquire": "1.7.3",
    "rewire": "2.5.1",
    "socket.io-client": "1.3.7"
  }
}
