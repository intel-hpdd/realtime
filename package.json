{
  "name": "realtime",
  "version": "3.0.0",
  "description": "Realtime communications layer between IML GUI and API",
  "scripts": {
    "test": "JASMINE_CONFIG_PATH=jasmine.json NODE_ENV=test jasmine",
    "integration": "JASMINE_CONFIG_PATH=jasmine.json NODE_ENV=test jasmine test/integration/**/*-test.js",
    "unit": "JASMINE_CONFIG_PATH=jasmine.json NODE_ENV=test jasmine test/unit/**/*-test.js",
    "jshint": "jshint --c ./.jshintrc --exclude-path ./.jshintignore .",
    "jscs": "jscs ."
  },
  "pre-commit": [
    "jscs",
    "jshint",
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/intel-js/realtime.git",
    "unit": "JASMINE_CONFIG_PATH=jasmine.json NODE_ENV=test jasmine test/unit/**/*-test.js"
  },
  "author": "IML Team",
  "license": "MIT",
  "dependencies": {
    "@intel-js/fp": "1.0.4",
    "@intel-js/obj": "1.0.3",
    "bunyan": "1.4.0",
    "highland": "2.5.1",
    "json-mask": "0.3.4",
    "jsonschema": "1.0.2",
    "lodash-mixins": "file:../../isomorphic/lodash-mixins",
    "nconf": "0.7.1",
    "req": "file:../req",
    "router": "file:../router",
    "socket.io": "1.3.6",
    "srcmap-reverse": "file:../srcmap-reverse",
    "through": "file:../through"
  },
  "devDependencies": {
    "jasmine": "2.3.1",
    "jasmine-n-matchers": "2.0.2",
    "jscs": "2.1.0",
    "jshint": "2.8.0",
    "krusty-jasmine-reporter": "1.0.0",
    "pre-commit": "1.1.1",
    "rewire": "2.3.4",
    "stub-daddy": "file:../stub-daddy"
  }
}
