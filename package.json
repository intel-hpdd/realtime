{
  "name": "@iml/realtime",
  "version": "5.1.0",
  "main": "dist/bundle.js",
  "description": "Realtime communications layer between MFL GUI and API",
  "config": {
    "port": "8888",
    "manager_url": "https://localhost:443/"
  },
  "scripts": {
    "app": "node ./dist/bundle.js",
    "eslint": "eslint ./",
    "prejest": "sendProjFile",
    "jest": "jest --watch",
    "test": "dotnet fable npm-run jest",
    "rollup": "rollup -c rollup-config.js",
    "postversion": "dotnet fable npm-run rollup"
  },
  "pre-commit": [
    "eslint",
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:intel-hpdd/realtime.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "MFL Team",
  "license": "MIT",
  "dependencies": {
    "@mfl/router": "6.0.1"
  },
  "devDependencies": {
    "@mfl/stub-daddy": "2.0.3",
    "babel-eslint": "7.2.3",
    "babel-plugin-transform-object-rest-spread": "6.23.0",
    "babel-preset-env": "1.5.1",
    "eslint": "3.19.0",
    "eslint-config-prettier": "2.1.1",
    "eslint-plugin-prettier": "2.1.1",
    "jest": "20.0.4",
    "jest-fable-preprocessor": "1.2.6",
    "pg": "6.2.3",
    "pre-commit": "1.2.2",
    "prettier": "1.3.1",
    "rollup": "0.41.6",
    "rollup-plugin-babel": "2.7.1",
    "rollup-plugin-bundle-size": "1.0.1",
    "rollup-plugin-commonjs": "8.0.2",
    "rollup-plugin-fable": "1.0.4",
    "rollup-plugin-node-resolve": "3.0.0",
    "rollup-plugin-re": "1.0.2",
    "rollup-watch": "3.2.2",
    "socket.io": "2.0.2",
    "socket.io-client": "2.0.2"
  }
